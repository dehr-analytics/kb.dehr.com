{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "template-dehr/**",
          "assets/**"
        ]
      }
    ],
    "output": "_site",
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
       "_enableSearch": "true",
      "_appName": "",
      "_appTitle": "DEHR Analytics",
      "pdf": false,
      "_appLogoPath": "/template-dehr/dehr.png",
      "_appFooter": "&copy; <a href='https://www.dehr.com'>DEHR Analytics</a>",
      "_enableNewTab": true,
      "_appFaviconPath": "/template-dehr/favicon.ico",
      "_googleAnalyticsTagId": "G-YKKMKFFJCP"
    },
    "template": [
     "default",
     "modern",
     "template-dehr"
    ]
  }
}
